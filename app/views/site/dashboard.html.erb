<%= content_for :page_title do 'Dashboard' end %>

<h1>Dashboard</h1>
</br>
<div class="row-fluid">
  <div class="span7">
    <div class="tasks">
      <h4 id="latest-task">Latest Tasks</h4></br>
      <% @tasks.each do |t| %>
        <%= render 'tasks/show', task: t, worktime: Worktime.new %>
      <% end %>
    </div>
  </div>

  <div class="projects span4 offset1">
    <%= render 'projects/list' %>
  </div>
</div>
