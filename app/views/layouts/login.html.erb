<!DOCTYPE html>

<html>

<head>
  <title>TicTracking - <%= yield :page_title %></title>

  <meta charset="utf-8" />

  <%= stylesheet_link_tag 'application', media: 'all' %>
  <%= javascript_include_tag 'application',  defer: 'defer' %>
  <%= csrf_meta_tags %>
</head>

<body>

  <!-- HEADER BEGIN -->
  <%= render 'layouts/top_navbar' %>
  <!-- HEADER END -->


  <!-- CONTENT BEGIN -->
  <section id="app-content" class="container <%= yield :page_id %>">
    <div class="row-fluid">
      <div class="span12">
        <section class="span5  offset3 well">
          <%= flash_message %>
          <%= yield %>
        </section>
      </div>
    </div>
  </section>
  <!-- CONTENT END -->

  <!-- FOOTER BEGIN -->
  <footer id="app-content" class="text-center">
    <p>&copy 2013, <a href="http://algorich.com" target="_blank">Algorich</a></p>
  </footer>
  <!-- FOOTER END -->

</body>

</html>